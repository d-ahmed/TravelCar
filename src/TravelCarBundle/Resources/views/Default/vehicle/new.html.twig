{% extends '@FOSUser/layout.html.twig' %}
{% trans_default_domain 'TravelCarBundle' %}
{% block title %}{{ 'label.title.myVehicles'|trans }}{% endblock %}

{% block fos_user_content %}
    <h1>Vehicle creation</h1>
    <div class="col-md-4 col-sm-4">
        {{ form_start(form) }}
            {{ form_widget(form) }}
            <input class="btn btn-default" type="submit" value="Create" />
        {{ form_end(form) }}
    </div>
    <div class="col-md-12">
        <a href="{{ path('vehicle_index') }}">Back to the list</a>
    </div>


{% endblock %}
